      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist"
    },
     "compileOnSave": true,
     "include": [
        "./src/*"
      ],
      "exclude": [
        "./node_modules/",
        "./dist/"
      ],
    "lib": ["es2015"]
  }